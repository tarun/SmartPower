    <service name="SmartMeterForDevices">
      <implementation-class>home.power.smartmeter.SmartMeterForDevices</implementation-class>
    </service>
    <service name="SmartMeterForPowerProvider">
      <implementation-class>home.power.smartmeter.SmartMeterForPowerProvider</implementation-class>
    </service>
  </services>
  <clients>
    <client name="PowerProviderForSmartMetersService">
      <wsdl-url>http://localhost:8080/PowerProvider/PowerProviderForSmartMetersService?WSDL</wsdl-url>
      <local-wsdl-file>localhost_8080/PowerProvider/PowerProviderForSmartMetersService.wsdl</local-wsdl-file>
      <package-name>utility.power.provider</package-name>
      <catalog-file>catalog.xml</catalog-file>
    </client>
  </clients>
</jax-ws>
